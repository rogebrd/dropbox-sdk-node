{
  "name": "dropbox-sdk-node",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "typings": "types/dropbox-sdk.d.ts",
  "type": "module",
  "scripts": {
    "build": "npm run test && npm run lint && npm run prettier && npm run build-src",
    "build-src": "babel src -d dist/src; babel lib -d dist/lib; babel index.js -d dist",
    "lint": "eslint --ext .js,.jsx,.ts .",
    "lint-fix": "eslint --fix --ext .js,.jsx,.ts .",
    "prettier": "prettier --check .",
    "prettier-fix": "prettier --write .",
    "report-mocha": "nyc mocha test/*.js",
    "test": "npm run test:typescript && npm run test:unit && npm run test:integration",
    "test:typescript": "tsc --build test/types",
    "test:integration": "./node_modules/.bin/mocha --timeout 10000 --require @babel/register test/integration/**/*.js",
    "test:unit": "./node_modules/.bin/mocha --require @babel/register test/unit/**/*.js",
    "report": "nyc -r test -r lcov report",
    "generate-docs": "jsdoc -c ./.jsdoc.json",
    "publish-docs": "rm -rf docs && npm run generate-docs && gh-pages -d docs"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@babel/register": "^7.10.5",
    "@types/node": "^14.0.23",
    "@typescript-eslint/eslint-plugin": "^3.6.1",
    "@typescript-eslint/parser": "^3.6.1",
    "chai": "^4.2.0",
    "eslint": "^7.4.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "gh-pages": "^2.2.0",
    "ink-docstrap": "^1.3.2",
    "jsdoc": "^3.6.4",
    "mocha": "^8.0.1",
    "nyc": "^15.1.0",
    "prettier": "^2.0.5",
    "sinon": "^9.0.2",
    "typescript": "^3.9.6"
  },
  "dependencies": {
    "node-fetch": "^2.6.0"
  }
}
