{
  "name": "dropbox-sdk-js",
  "version": "6.0.0",
  "description": "The Dropbox JavaScript SDK is a lightweight, promise based interface to the Dropbox v2 API that works in both nodejs and browser environments.",
  "main": "index.js",
  "browser": "dist/index.js",
  "typings": "types/dropbox-sdk.d.ts",
  "scripts": {
    "start": "node examples/javascript/server.js",
    "build": "babel src -d dist/src; babel lib -d dist/lib; babel index.js -d dist",
    "lint": "eslint --ext .js,.jsx,.ts .",
    "lint-fix": "eslint --fix --ext .js,.jsx,.ts .",
    "test": "npm run test:typescript && npm run test:unit",
    "test:typescript": "tsc --build test/types",
    "test:integration": "./node_modules/.bin/mocha --timeout 10000 --require @babel/register test/integration/**/*.js",
    "test:unit": "./node_modules/.bin/mocha --require @babel/register test/unit/**/*.js",
    "report": "nyc report --reporter=lcov --reporter=text",
    "generate-docs": "jsdoc -c ./.jsdoc.json",
    "publish-docs": "rm -rf docs && npm run generate-docs && gh-pages -d docs",
    "coverage:unit": "nyc --reporter=lcov npm run test:unit",
    "coverage:integration": "nyc --reporter=lcov npm run test:integration"
  },
  "files": [
    "*.md",
    "docs",
    "dist",
    "lib",
    "es",
    "generator",
    "src",
    "conf.json",
    "*.config.js"
  ],
  "keywords": [
    "dropbox",
    "files",
    "sync",
    "sdk",
    "client"
  ],
  "homepage": "https://github.com/dropbox/dropbox-sdk-js#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dropbox/dropbox-sdk-js.git"
  },
  "bugs": {
    "url": "https://github.com/dropbox/dropbox-sdk-js/issues"
  },
  "license": "MIT",
  "directories": {
    "doc": "docs",
    "example": "examples",
    "test": "test"
  },
  "engines": {
    "node": ">=0.10"
  },
  "contributors": [
    "Brad Rogers <brad12rogers@gmail.com>",
    "Andrew Lawson <>",
    "James Sidhu <james.thomas.sidhu@gmail.com>",
    "John Vilk <jvilk@cs.umass.edu>",
    "Steve Klebanoff <steve.klebanoff@gmail.com>",
    "Bohdan Tereta <Bohdan.Tereta@gmail.com>"
  ],
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@babel/register": "^7.10.5",
    "@rollup/plugin-babel": "^5.2.1",
    "@types/node": "^14.0.23",
    "@typescript-eslint/eslint-plugin": "^3.6.1",
    "@typescript-eslint/parser": "^3.6.1",
    "babel-plugin-istanbul": "^6.0.0",
    "chai": "^4.2.0",
    "eslint": "^7.4.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.0",
    "express": "^4.17.1",
    "express-urlrewrite": "^1.3.0",
    "gh-pages": "^2.2.0",
    "ink-docstrap": "^1.3.2",
    "jsdoc": "^3.6.4",
    "mocha": "^8.0.1",
    "nyc": "^15.1.0",
    "rollup-endpoint": "^0.2.2",
    "sinon": "^9.0.2",
    "typescript": "^3.9.6"
  },
  "dependencies": {
    "node-fetch": "^2.6.0"
  }
}
